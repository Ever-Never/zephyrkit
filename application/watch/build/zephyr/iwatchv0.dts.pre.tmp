  label = "MMC5603";
  reg = <0x30>;
  pwr-gpios = <&gpio0 13 (0 << 0)>;
 };
};
&i2c2 {
    status = "okay";
};
&i2c3 {
    status = "okay";
 cw6305@0B {
  compatible = "cellwise,cw6305";
  label = "CW6305";
  reg = <0x0B>;
  int-gpios = <&gpio0 16 (1 << 0)>;
 };
};
&i2c4 {
    status = "okay";
};
&spi0 {
 status = "okay";
 cs-gpios = <&gpio0 3 (1 << 0)>;
 icm42607@0 {
   compatible = "invensense,icm42607";
   int-gpios = <&gpio0 17 (1 << 0)>;
   label = "ICM42607";
   reg = <0>;
   spi-max-frequency = <10000000>;
  };
};
&i2c5 {
    status = "okay";
};
&mspi0 {
    status = "okay";
};
&mspi1 {
    status = "okay";
};
&adc1 {
    status = "okay";
};
&channel {
    status = "okay";
