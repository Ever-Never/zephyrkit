&nvic {
 arm,num-irq-priority-bits = <3>;
};
/ {
 model = "AmbiqMicro apollo3p board";
 compatible = "ambiq,apollo3p";
 chosen {
  zephyr,console = &uart0;
  zephyr,shell-uart = &uart0;
  zephyr,flash = &flash0;
  zephyr,sram = &sram0;
  zephyr,ccm = &ccm0;
  zephyr,flash-controller = &flash;
  zephyr,code-partition = &slot0_partition;
  psram = &psram0;
 };
 psram0: memory@55000000 {
  compatible = "aps1604,psram";
  label = "psram";
  reg = <0x56000000 ((2048) * 1024)>;
 };
 power-states {
  normal_sleep: state0 {
   compatible = "zephyr,power-state";
   power-state-name = "suspend-to-idle";
   substate-id = <0>;
   min-residency-us = <500>;
  };
  deep_sleep: state1 {
   compatible = "zephyr,power-state";
   power-state-name = "suspend-to-idle";
   substate-id = <1>;
   min-residency-us = <2000>;
  };
 };
 soc {
  flash-controller@40018000 {
   flash0: flash@00000000 {
    reg = <0x00000000 ((2048) * 1024)>;
   };
  };
 };
    mspi-nor {
   compatible = "gd,mspi-nor";
   label = "MSPI-NOR";
   partitions {
    compatible = "fixed-partitions";
    #address-cells = <1>;
    #size-cells = <1>;
    resource_partition: partition@0 {
     label = "resource_img";
     reg = <0x0 ((8*1024) * 1024)>;
    };
    watch_face_theme1_partition: partition@800000 {
     label = "watch_face_1";
     reg = <0x800000 ((2048) * 1024)>;
    };
    watch_face_theme2_partition: partition@A00000 {
     label = "watch_face_2";
     reg = <0xA00000 ((2048) * 1024)>;
    };
    watch_face_theme3_partition: partition@C00000 {
     label = "watch_face_3";
     reg = <0xC00000 ((2048) * 1024)>;
    };
    watch_face_theme4_partition: partition@E00000 {
     label = "watch_face_4";
     reg = <0xE00000 ((2048) * 1024)>;
    };
    watch_face_theme5_partition: partition@1000000 {
     label = "watch_face_5";
     reg = <0x1000000 ((2048) * 1024)>;
    };
    storage_partition: partition@1200000 {
     label = "storage";
     reg = <0x1200000 ((2048) * 1024)>;
    };
    update_partition: partition@1400000 {
     label = "update_img";
     reg = <0x1400000 ((2048) * 1024)>;
    };
    file_system: partition@1600000 {
     label = "fs";
     reg = <0x1600000 ((10*1024) * 1024)>;
    };
   };
  };
 channel: channel {
  compatible = "ambiq,channel";
  io-channels = <&adc1 16>;
  io-channel-names = "ADCSE0";
  status = "disabled";
  label = "channel";
 };
 pwmmotor {
  compatible = "pwm-motors";
  label = "MOTOR";
  motor: motor {
      pwms = <&pwm6 0 1 (0 << 0)>;
  };
 };
 gpio_keys {
  compatible = "gpio-buttons";
  label = "buttons";
  control_button: button-1 {
   label = "Control";
   gpios = <&gpio2 7 (1 << 0)>;
   code = <0x1b12>;
  };
  home_button: button-2 {
   label = "Home";
   gpios = <&gpio0 16 (1 << 0)>;
   code = <0x1b04>;
  };
 };
};
&cpu0 {
 cpu-power-states = <&normal_sleep &deep_sleep>;
};
&flash0 {
 partitions {
  compatible = "fixed-partitions";
  #address-cells = <1>;
  #size-cells = <1>;
  otp_partition: partition@0 {
   label = "otp";
   reg = <0x0 ((48) * 1024)>;
  };
  boot_partition: partition@c000 {
   label = "mcuboot";
   reg = <0x0000c000 ((192) * 1024)>;
  };
  slot0_partition: partition@3c000 {
   label = "image-0";
   reg = <0x0003c000 ((1808) * 1024)>;
  };
 };
};
&ctimer0 {
 status = "okay";
 clock_source = <((0xE << 1) & 0x0000003E)>;
 pwm0: pwm {
   ch0_pin = <25>;
   ch1_pin = <255>;
      status = "okay";
 };
};
&ctimer6 {
